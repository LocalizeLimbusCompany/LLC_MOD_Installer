        </Grid>
        <Grid x:Name="SettingsPage" Visibility="Collapsed" Panel.ZIndex="5" Margin="62,55,54,32" Grid.Column="2" Grid.Row="1">
            <Label Content="切换节点" HorizontalAlignment="Left" Margin="100,28,0,0" VerticalAlignment="Top" Height="41" Width="494" FontSize="22" Foreground="#FFFFEED4" FontFamily="/Fonts/#Source Han Sans CN" FontWeight="Medium"/>
            <ComboBox x:Name="NodeCombobox" Margin="304,35,70,330" FontFamily="/Fonts/#Source Han Sans CN" FontWeight="Medium" Foreground="Black" FontSize="16" SelectionChanged="NodeComboboxSelectionChanged" >
                <ComboBox.Clip>
                    <RectangleGeometry Rect="0,0,290,31" RadiusX="3" RadiusY="3"/>
                </ComboBox.Clip>
            </ComboBox>
            <Label Content="卸载模组" HorizontalAlignment="Left" Margin="100,80,0,0" VerticalAlignment="Top" Height="41" Width="494" FontSize="22" Foreground="#FFFFEED4" FontFamily="/Fonts/#Source Han Sans CN" FontWeight="Medium"/>
            <Image HorizontalAlignment="Center" Height="249" VerticalAlignment="Center" Width="524" Source="/Picture/SettingsLine.png"/>
            <Image x:Name="UninstallHover" IsHitTestVisible="False" Opacity="0" HorizontalAlignment="Left" Height="34" Margin="511,84,0,0" VerticalAlignment="Top" Width="80" Source="/Picture/SettingButtonHover.png"/>
            <Button HorizontalAlignment="Left" Margin="514,87,0,0" VerticalAlignment="Top" Height="28" Width="74" Style="{StaticResource NoHover}" Background="#FFFFEED4" Click="UninstallButtonClick">
                <Button.Clip>
                    <RectangleGeometry Rect="0,0,74,28" RadiusX="3" RadiusY="3"/>
